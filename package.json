{
  "name": "blizzapi",
  "version": "1.1.7",
  "description": "Flexible and feature-rich Node.js library for Blizzard Battle.net API",
  "homepage": "https://blizzapi.lukem.net",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "Łukasz Wójcik",
    "url": "https://www.lukem.net/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/lukemnet/blizzapi.git"
  },
  "bugs": {
    "url": "https://github.com/lukemnet/blizzapi/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/lukemnet/blizzapi/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "clean": "rimraf dist/*",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json",
    "build": "npm run lint && npm run test && npm run clean && tsc",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "automock": false
  },
  "dependencies": {
    "axios": "0.19.2",
    "base-64": "0.1.0"
  },
  "devDependencies": {
    "@types/base-64": "0.1.3",
    "@types/jest": "25.2.3",
    "@types/node": "14.0.24",
    "jest": "26.1.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.3",
    "tslint": "6.1.2",
    "tslint-config-airbnb": "5.11.2",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.1",
    "typescript": "3.9.7"
  },
  "keywords": [
    "blizzard",
    "battlenet",
    "battle-net",
    "battle-net-api",
    "blizzapi",
    "api",
    "starcraft2",
    "sc2",
    "wow",
    "worldofwarcraft",
    "world-of-warcraft",
    "d3",
    "diablo3"
  ]
}
