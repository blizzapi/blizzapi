{
  "name": "blizzapi",
  "version": "0.1.0",
  "description": "Feature-rich library for Blizzard Battle.net API",
  "homepage": "https://blizzapi.lukem.net",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "Łukasz Wójcik",
    "url": "https://www.lukem.net/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/lukemnet/blizzapi.git"
  },
  "bugs": {
    "url": "https://github.com/lukemnet/blizzapi/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/lukemnet/blizzapi/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "clean": "rimraf dist/*",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json",
    "build": "npm run lint && npm run test && npm run clean && tsc",
    "dev": "tsc -w"
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "./setupJest.ts"
    ]
  },
  "dependencies": {
    "axios": "0.19.0",
    "base-64": "0.1.0"
  },
  "devDependencies": {
    "@types/base-64": "0.1.3",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.2",
    "jest": "24.8.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "ts-jest": "24.0.2",
    "tslint": "5.18.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.1",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3"
  },
  "keywords": [
    "blizzard",
    "battlenet",
    "battle-net",
    "battle-net-api",
    "blizzapi",
    "api",
    "starcraft2",
    "sc2",
    "wow",
    "worldofwarcraft",
    "world-of-warcraft",
    "d3",
    "diablo3"
  ]
}
