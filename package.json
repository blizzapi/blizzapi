{
  "name": "blizzapi",
  "version": "2.0.4",
  "description": "Flexible and feature-rich library for easy access to Blizzard Battle.net APIs",
  "homepage": "https://blizzapi.lukem.net",
  "main": "dist/blizzapi.js",
  "browser": "dist/blizzapi.umd.js",
  "typings": "dist/blizzapi.d.ts",
  "author": {
    "name": "BlizzAPI + Contributors",
    "url": "https://github.com/blizzapi"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/blizzapi/blizzapi.git"
  },
  "bugs": {
    "url": "https://github.com/blizzapi/blizzapi/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blizzapi/blizzapi/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "clean": "rimraf dist/*",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts",
    "compile": "rollup -c",
    "build": "npm run lint && npm run test && npm run clean && npm run compile",
    "dev": "tsc -w --project tsconfig.json",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "esbuild": "^0.14.6",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-airbnb-typescript-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-esbuild": "^4.7.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "keywords": [
    "blizzard",
    "battlenet",
    "battle-net",
    "battle-net-api",
    "blizzapi",
    "api",
    "starcraft2",
    "sc2",
    "wow",
    "worldofwarcraft",
    "world-of-warcraft",
    "d3",
    "diablo3"
  ]
}
